{
  "name": "phayne-io/php-exception",
  "description": "Set of commonly used PHP Exceptions",
  "type": "library",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Julien Guittard",
      "email": "julien.guittard@me.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "fig/http-message-util": "^1.1",
    "laminas/laminas-json": "^3.5"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.4",
    "malukenho/docheader": "^1.0",
    "php-coveralls/php-coveralls": "^v2.5",
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^4.30"
  },
  "autoload": {
    "psr-4": {
      "Phayne\\Exception\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhayneTest\\Exception\\": "test/"
    }
  },
  "scripts": {
    "test-html": "XDEBUG_MODE=coverage phpunit --colors=always -vvv --coverage-html build/html"
  },
  "support": {
    "source": "https://github.com/phayne-io/php-exception",
    "issues": "https://github.com/phayne-io/php-exception/issues",
    "rss": "https://github.com/phayne-io/php-exception/releases.atom",
    "wiki": "https://github.com/phayne-io/php-exception/wiki"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    }
  }
}
